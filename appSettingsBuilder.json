{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "customAppSettings": {
            "type": "array"
        },
        "funcDefaults": {
            "type": "array"
        },
        "referenced": {
            "type": "array"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [],
    "outputs": {
        "build": {
            "type": "array",
            "value": "[union(parameters('customAppSettings'), parameters('funcDefaults'), parameters('referenced'))]"
        }
    }
}